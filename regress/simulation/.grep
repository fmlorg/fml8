#!/bin/sh
#
# $Id$
#

tmp=/tmp/buffer$$
trap "rm -f $tmp" 0 1 3 15

cd ../.. || exit 1

find fml cpan -type f -print | egrep '\.pm$' > $tmp
find fml/libexec -type f -print | grep -v CVS >> $tmp
find regress -type f -print | grep -v CVS >> $tmp

for key in $*
do
   egrep $key `cat $tmp`
done

exit 0
