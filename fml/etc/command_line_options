#
# $FML$
#
# map between programs and options
#

# program name	command line options
# --------------------------------------------------------------------
#

# libexec/
command		ctladdr! debug! help! params=s -c=s
digest		ctladdr! debug! help! params=s -c=s
distribute	ctladdr! debug! help! params=s -c=s
error		ctladdr! debug! help! params=s -c=s
mead		ctladdr! debug! help! params=s -c=s

# bin/
fml		debug! help! force! params=s -c=s
fmladdr		debug! help! -c=s n!
fmlalias	debug! help! -c=s n!
fmlconf		debug! help! params=s -c=s n!
fmldoc		debug! help! params=s -c=s v! t! u! m! l!
fmlerror	debug! help!
fmlhtmlify	debug! help! -I=s
fmlspool	debug! help! -I=s convert! style=s srcdir=s
fmlsummary	debug! help! -c=s n!
fmlsuper	debug! help!
fmlthread	debug! help! article_id_max=i spool_dir=s base_url=s msg_base_url=s reverse! params=s -f=s -c=s
makefml		debug! help! force! params=s -c=s

# cgi-bin/
config.cgi		
fmlthread.cgi		
thread.cgi		
threadview.cgi		

# demo
fmlsch		debug! help! -D=s -F=s -m=s a! h!
fmlsch.cgi		

# compatibility
fml.pl		ctladdr! debug! help! params=s -c=s

# test
loader		ctladdr! debug! help! params=s -c=s
