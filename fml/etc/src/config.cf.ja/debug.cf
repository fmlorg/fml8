=head1	デバッグに役立つ機能

=head2	キャッシュ


# Descriptions: 入力されるメールをキャッシュするか？
#      History: $NUM_LOG_MAIL
#        Value: yes / no
#     Examples: y
use_incoming_message_cache	=	yes


# Descriptions: 入力されるメールを最大何通キャッシュするか
#      History: $NUM_LOG_MAIL
#        Value: NUM
#     Examples: 128
incoming_message_cache_size	=	128
